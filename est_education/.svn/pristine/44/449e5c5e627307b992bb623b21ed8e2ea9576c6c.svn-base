package com.authority.vo;

import common.base.vo.BaseVo;

public class AuthorityVo extends BaseVo {
	String authCd;     //권한코드
	String authNm;     //권한명
	String useYn;  //사용여부
	String note;   //비고
	
	public AuthorityVo() {
		super();
	}
	
	public AuthorityVo(String authCd, String authNm, String useYn, String note) {
		super();
		this.authCd = authCd;
		this.authNm = authNm;
		this.useYn = useYn;
		this.note = note;
	}
	
	public String getAuthCd() {
		return authCd;
	}
	public void setAuthCd(String authCd) {
		this.authCd = authCd;
	}
	public String getAuthNm() {
		return authNm;
	}
	public void setAuthNm(String authNm) {
		this.authNm = authNm;
	}
	
	public String getUseYn() {
		return useYn;
	}
	public void setUseYn(String useYn) {
		this.useYn = useYn;
	}
	public String getNote() {
		return note;
	}
	public void setNote(String note) {
		this.note = note;
	}
	
	@Override
	public String toString() {
		return "AuthorityVo [authCd=" + authCd + ", authNm=" + authNm + ", useYn=" + useYn + ", note="
				+ note + "]";
	}
}
