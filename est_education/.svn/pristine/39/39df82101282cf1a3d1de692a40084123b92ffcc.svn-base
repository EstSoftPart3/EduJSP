package com.program.vo;

import common.base.vo.BaseVo;

public class ProgramVo extends BaseVo {
	
	String programGrpNm; // 프로그램 그룹
	String programGrpCd; // 프로그램 그풉 코드
	String programCd; //프로그램 코드
	String programNm; //프로그램 이름
	String srcPath; //프로그램 경로
	String isUse; //프로그램 사용 여부
	String note; //프로그램 비고

	//기본생성자
	public ProgramVo() { super(); }

	public String getProgramGrpNm() {
		return programGrpNm;
	}

	public void setProgramGrpNm(String programGrpNm) {
		this.programGrpNm = programGrpNm;
	}

	public String getProgramGrpCd() {
		return programGrpCd;
	}

	public void setProgramGrpCd(String programGrpCd) {
		this.programGrpCd = programGrpCd;
	}

	public String getProgramCd() {
		return programCd;
	}

	public void setProgramCd(String programCd) {
		this.programCd = programCd;
	}

	public String getProgramNm() {
		return programNm;
	}

	public void setProgramNm(String programNm) {
		this.programNm = programNm;
	}

	public String getSrcPath() {
		return srcPath;
	}

	public void setSrcPath(String srcPath) {
		this.srcPath = srcPath;
	}

	public String getIsUse() {
		return isUse;
	}

	public void setIsUse(String isUse) {
		this.isUse = isUse;
	}

	public String getNote() {
		return note;
	}

	public void setNote(String note) {
		this.note = note;
	}

	public ProgramVo(String programGrpNm, String programGrpCd, String programCd, String programNm, String srcPath,
			String isUse, String note) {
		super();
		this.programGrpNm = programGrpNm;
		this.programGrpCd = programGrpCd;
		this.programCd = programCd;
		this.programNm = programNm;
		this.srcPath = srcPath;
		this.isUse = isUse;
		this.note = note;
	}

	@Override
	public String toString() {
		return "ProgramVo [programGrpNm=" + programGrpNm + ", programGrpCd=" + programGrpCd + ", programCd=" + programCd
				+ ", programNm=" + programNm + ", srcPath=" + srcPath + ", isUse=" + isUse + ", note=" + note + "]";
	}

	


}
